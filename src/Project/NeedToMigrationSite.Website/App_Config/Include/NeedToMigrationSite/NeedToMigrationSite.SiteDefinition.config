<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
    <sitecore>
        <domainManager>
            <domains>
                <domain id="msocial" type="Sitecore.Security.Domains.Domain, Sitecore.Kernel">
                    <param desc="name">$(id)</param>
                    <ensureAnonymousUser>false</ensureAnonymousUser>
                </domain>
            </domains>
        </domainManager>
        <sites>
            <site name="needtomigrationsite"
			      patch:after="site[@name='website']"
			      targetHostName="needtomigrationsite.local"
			      enableTracking="true"
			      virtualFolder="/"
			      physicalFolder="/"
			      rootPath="/sitecore/content/NeedToMigrationSite"
			      startItem="/Home"
			      database="web"
			      domain="extranet"
			      allowDebug="true"
			      cacheHtml="true"
			      htmlCacheSize="200MB"
			      registryCacheSize="0"
			      viewStateCacheSize="0"
			      xslCacheSize="25MB"
			      filteredItemsCacheSize="10MB"
			      enablePreview="true"
			      enableWebEdit="true"
			      enableDebugger="true"
			      disableClientData="false"
			      cacheRenderingParameters="true"
			      renderingParametersCacheSize="10MB"
			      enableItemLanguageFallback="true"
			      enableFieldLanguageFallback="true" />
        </sites>
        <cacheSizes>
            <sites>
                <msocial>
                    <html>50MB</html>
                    <registry>0</registry>
                    <viewState>0</viewState>
                    <xsl>25MB</xsl>
                </msocial>
            </sites>
        </cacheSizes>
        <settings>
            <setting name="Preview.DefaultSite">
                <patch:attribute name="value" value="needtomigrationsite" />
            </setting>
        </settings>
    </sitecore>
</configuration>